#:kivy 1.11.1

<MyGraph>:
    Graph:
        size: root.size
        background_color: 0.8, 0.8, 0.8, 1
        border_color: 0, 0.1, 0.1, 1
        xlabel: 'Date/Heure'
        ylabel: 'Valeur du capteur'
        x_ticks_minor: 5
        x_ticks_major: 25
        y_ticks_major: 1
        y_grid_label: True
        x_grid_label: True
        padding: 5
        x_grid: True
        y_grid: True
        xmin: 0
        xmax: 100
        ymin: -1
        ymax: 1
        tick_color: (1, 0, 0, 1)
        label_options: {'color': (0.5, 0.5, 0, 1)}

<GraphExample>:
    BoxLayout:
        graph_id: graph_id
        MyGraph:
            id: graph_id
