#:kivy 1.11.1

<GraphExample>:
    BoxLayout:
        orientation: 'vertical'
        padding: 20

        Graph:
            size_hint_y: 0.8
            id: graph
            background_color: 0, 1, 1, 1
            border_color: 1, 1, 0, 1
            xlabel: 'X'
            ylabel: 'Y'
            x_ticks_minor: 5
            x_ticks_major: 25
            y_ticks_major: 1
            y_grid_label: True
            x_grid_label: True
            padding: 5
            x_grid: True
            y_grid: True
            xmin:0
            xmax:100
            ymin:-1
            ymax:1
